
<% if column == "order_created_at" %>
    <div class="chart<%= " hidden" if hidden %>" id="<%= column %>">
        <%= line_chart chart.group_by_day(:order_created_at).count, xtitle: "Time", ytitle: "Number of Order", colors: ["#b00", "#666"], stacked: true , discrete: true, legend: false, download: true%>
    </div>
